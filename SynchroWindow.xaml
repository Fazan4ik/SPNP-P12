<Window x:Class="SPNP_P12.SynchroWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SPNP_P12"
        mc:Ignorable="d"
        Closed="Window_Closed"
        Title="SynchroWindow" Height="450" Width="800">
    <StackPanel Orientation="Vertical" Background="Yellow">
        <Label Content="Synchro"/>
        <Button x:Name="StartButton" Content="Start" Margin="5 10 5 10" Width="70" HorizontalAlignment="Left" Click="StartButton_Click"/>
        <TextBlock x:Name="LogtextBlock" Margin="5 10 5 10" HorizontalAlignment="Left"/>
    </StackPanel>
</Window>
